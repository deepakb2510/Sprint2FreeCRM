</contact>

	<companies>
		<email>email</email>
		<pass>password</pass>
		<submit>//div[@class='ui fluid large blue submit button']</submit>
		<createbtn>//button[normalize-space()='Create']</createbtn>
		<createform>/html/body/div[1]/div/div[2]/div[2]/div/div[1]/div[1]</createform>
		<companyname>/html[1]/body[1]/div[1]/div[1]/div[2]/div[2]/div[1]/div[2]/form[1]/div[1]/div[1]/div[1]/div[1]/div[1]/input[1]</companyname>
