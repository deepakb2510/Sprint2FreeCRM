<freecrm>
	<contact>
	<email>/html/body/div[1]/div/div/form/div/div[1]/div/input</email>
	<pass>password</pass>
	<login>//*[@id="ui"]/div/div/form/div/div[3]</login>
	<contactbutton>/html/body/div[1]/div/div[1]/div[3]/a</contactbutton>
