		<companyformcancel>//button[@class='ui button']</companyformcancel>
		<savebtn>//*[@id=\"dashboard-toolbar\"]/div[2]/div/button[2]</savebtn>
		<saveerr>#main-content > div > div.ui.fluid.container.main-content > form > div:nth-child(1) > div:nth-child(1) > div > label > span</saveerr>
		<mobileno>//input[@placeholder='Number']</mobileno>
		<delete>/html/body/div[1]/div/div[2]/div[2]/div/div[2]/div/table/tbody/tr[1]/td[4]/div/button</delete>
