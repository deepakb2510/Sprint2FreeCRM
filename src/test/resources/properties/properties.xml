		<!--Filter companies  -->
		<showfilter>.linkedin:nth-child(2)</showfilter>
		<search>.search > .search</search>
		<selectname>.visible > .selected</selectname>
		<opreator>.field .default</opreator>
		<operatorselect>.visible > .selected > .text</operatorselect>
		<value>value</value>
		<savesearch>.small:nth-child(4)</savesearch> 
		<clear>.ban</clear>
		<filtersearch>/html[1]/body[1]/div[1]/div[1]/div[2]/div[2]/div[1]/div[2]/form[1]/div[1]/div[2]/div[1]/button[5]</filtersearch>
		
		<cancelfilter>/html/body/div[3]/div/div[3]/button[1]</cancelfilter>
	
